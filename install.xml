<?xml version="1.0" encoding="utf-8" ?>
<module type="plugin" name="agreement">
    <title>Registration agreement</title>
    <summary>Add site rules for register page.</summary>
    <author>Intelliants LLC</author>
    <contributor>Intelliants LLC</contributor>
    <version>4.2.0</version>
    <date>2018-03-07</date>
    <compatibility>4.2.0</compatibility>

    <phrases>
        <phrase key="agreement" category="frontend">You have to accept our rules.</phrase>
        <phrase key="agree" category="frontend">I agree</phrase>
        <phrase key="not_agree" category="frontend">I do not agree</phrase>
    </phrases>

    <hooks>
        <hook name="phpCoreCodeAfterAll" pages="registration" page_type="front" filename="hook.phpCoreCodeAfterAll.php"><![CDATA[]]></hook>
    </hooks>
</module>